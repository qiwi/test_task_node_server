    "@types/chai-subset": "^1.3.1",
    "@types/cucumber": "^4.0.4",
    "@types/node-fetch": "^2.1.4",
    "apidoc": "^0.17.5",
    "chai": "^4.2.0",
    "chai-subset": "^1.6.0",
    "cucumber": "^5.1.0",
    "jsonschema": "^1.2.4",

    "@types/request-promise": "^4.1.44",
    "apidoc": "^0.17.7",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "source-map-support": "^0.5.12",
